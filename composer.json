{
	"name": "algoritmika/unlogged-order-tracking-for-woocommerce",
	"description": "Makes it easier for unlogged users to access an WooCommerce order",
	"keywords": [
		"wordpress",
		"plugin",
		"woocommerce",
		"order",
		"tracking",
		"orders"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomas Anbinderis",
			"email": "anbinderis@gmail.com"
		},
		{
			"name": "Pablo Pacheco",
			"email": "pablo.sg.pacheco@gmail.com"
		}
	],
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true
	},
	"type": "wordpress-plugin",
	"extra": {
		"installer-paths": {
			"vendor/{$vendor}/{$name}": [
				"type:wordpress-plugin",
				"type:wordpress-muplugin"
			]
		}
	},
	"autoload": {
		"classmap": [
			"classes/"
		]
	}
}